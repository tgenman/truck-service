<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>

<!DOCTYPE html>

<html>

<head>
    <title>Student Registration Form</title>
</head>

<body>

<form action="newTruck" method="GET">

    <br><br>

    <input type="text" name="licensePlate"
           placeholder="No. of a license plate" />
    <br><br>
    <input type="text" name="labourTime"
           placeholder="What is a labour time?" />
    <br><br>

    <input type="text" name="capacity"
           placeholder="How much is capacity for this truck" />
    <br><br>

    <input type="text" name="operable"
           placeholder="Is it operable?" />
    <br><br>

    <input type="text" name="city"
           placeholder="What is a current location" />
    <br><br>


    <input type="submit" />

</form>


</body>

</html>












